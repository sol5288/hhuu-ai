{
  "extends": ["plugin:vue/vue3-recommended", "@vue/typescript/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "eslint-comments/no-unlimited-disable": "off",
    "curly": ["error", "all"],
    "vue/component-tags-order": [
      "error",
      {
        "order": ["route", "script", "template", "style"]
      }
    ]
  }
}
